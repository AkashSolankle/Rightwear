<?xml version="1.0" encoding="utf-8"?>
<android.widget.RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/scr"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/b8"
    tools:context=".Order">

    <Button
        android:id="@+id/b3"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="31dp"
        android:background="@android:drawable/checkbox_off_background"
        android:text="Place Order" />

    <EditText
        android:id="@+id/editText6"
        android:layout_width="120dp"
        android:layout_height="35dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="17dp"
        android:layout_marginTop="134dp"
        android:ems="10"
        android:hint="Neck"
        android:inputType="number"
        android:textColor="@android:color/background_dark"
        android:textSize="12sp" />

    <EditText
        android:id="@+id/editText7"
        android:layout_width="120dp"
        android:layout_height="35dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/editText6"
        android:layout_marginEnd="34dp"
        android:ems="10"
        android:hint="Shoulder"
        android:inputType="number"
        android:textColor="@android:color/background_dark"
        android:textSize="12sp" />

    <EditText
        android:id="@+id/editText8"
        android:layout_width="120dp"
        android:layout_height="35dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/editText6"
        android:layout_marginTop="172dp"
        android:ems="10"
        android:hint="chest"
        android:inputType="number"
        android:textColor="@android:color/background_dark"
        android:textSize="12sp" />

    <EditText
        android:id="@+id/editText9"
        android:layout_width="120dp"
        android:layout_height="35dp"
        android:layout_alignStart="@+id/editText7"
        android:layout_alignTop="@+id/editText8"
        android:ems="10"
        android:hint="Arm Hole"
        android:inputType="number"
        android:textColor="@android:color/background_dark"
        android:textSize="12sp" />

    <EditText
        android:id="@+id/editText10"
        android:layout_width="120dp"
        android:layout_height="35dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/editText6"
        android:layout_marginTop="213dp"
        android:ems="10"
        android:hint="Waist"
        android:inputType="number"
        android:textColor="@android:color/background_dark"
        android:textSize="12sp" />

    <EditText
        android:id="@+id/editText11"
        android:layout_width="120dp"
        android:layout_height="35dp"
        android:layout_alignStart="@+id/editText7"
        android:layout_alignTop="@+id/editText10"
        android:ems="10"
        android:hint="Stomach"
        android:inputType="number"
        android:textColor="@android:color/background_dark"
        android:textSize="12sp" />

    <EditText
        android:id="@+id/editText12"
        android:layout_width="120dp"
        android:layout_height="35dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/editText6"
        android:layout_marginBottom="227dp"
        android:ems="10"
        android:hint="Hip"
        android:inputType="number"
        android:textColor="@android:color/background_dark"
        android:textSize="12sp" />

    <EditText
        android:id="@+id/editText13"
        android:layout_width="120dp"
        android:layout_height="35dp"
        android:layout_alignStart="@+id/editText7"
        android:layout_alignTop="@+id/editText12"
        android:ems="10"
        android:hint="Thigh"
        android:inputType="number"
        android:textColor="@android:color/background_dark"
        android:textSize="12sp" />

    <EditText
        android:id="@+id/editText14"
        android:layout_width="120dp"
        android:layout_height="35dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/editText6"
        android:layout_marginBottom="186dp"
        android:ems="10"
        android:hint="Calf"
        android:inputType="number"
        android:textColor="@android:color/background_dark"
        android:textSize="12sp" />

    <EditText
        android:id="@+id/editText15"
        android:layout_width="120dp"
        android:layout_height="35dp"
        android:layout_alignStart="@+id/editText7"
        android:layout_alignTop="@+id/editText14"
        android:ems="10"
        android:hint="Knee"
        android:inputType="number"
        android:textColor="@android:color/background_dark"
        android:textSize="12sp" />

    <EditText
        android:id="@+id/editText16"
        android:layout_width="120dp"
        android:layout_height="35dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/editText6"
        android:layout_marginBottom="149dp"
        android:ems="10"
        android:hint="Ankle"
        android:inputType="number"
        android:textColor="@android:color/background_dark"
        android:textSize="12sp" />

    <EditText
        android:id="@+id/editText17"
        android:layout_width="120dp"
        android:layout_height="35dp"
        android:layout_alignStart="@+id/editText7"
        android:layout_alignTop="@+id/editText16"
        android:ems="10"
        android:hint="Upper Bust"
        android:inputType="number"
        android:textColor="@android:color/background_dark"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/editText18"
        android:layout_width="131dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="81dp"
        android:ems="10"
        android:hint="Design Name"
        android:inputType="textPersonName"
        android:textColor="@android:color/background_dark"
        android:textSize="18sp" />

    <EditText
        android:id="@+id/editText19"
        style="@android:style/Widget.AutoCompleteTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="88dp"
        android:ems="10"
        android:hint="Enter Date"
        android:inputType="textPersonName"
        android:selectAllOnFocus="true"
        android:singleLine="true"
        android:textAppearance="@style/TextAppearance.AppCompat" />

</android.widget.RelativeLayout>